<!DOCTYPE html>
<html>

    <head>
        
        <meta charset="iso-8859-1">
        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>Moodle</title>
        
        <link rel="icon" href="../../favcon.png"><!-- Favicon-->

        <!-- #START# CSS's -->
        <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css"><!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css"><!-- Google Fonts -->
        <link href="../../plugins/bootstrap/css/bootstrap.css" rel="stylesheet"><!-- Bootstrap Core Css -->
        <link href="../../plugins/node-waves/waves.css" rel="stylesheet" /><!-- Waves Effect Css -->
        <link href="../../plugins/animate-css/animate.css" rel="stylesheet" /><!-- Animation Css -->
        <link href="../../plugins/jquery-datatable/skin/bootstrap/css/dataTables.bootstrap.css" rel="stylesheet"><!-- JQuery DataTable Css -->
        <link href="../../plugins/morrisjs/morris.css" rel="stylesheet" /><!-- Morris Chart Css-->
        <link href="../../css/style.css" rel="stylesheet"><!-- Custom Css -->
        <link href="../../css/themes/all-themes.css" rel="stylesheet" /><!-- AdminBSB Themes -->
        <!-- #END# CSS's -->

    </head>

    <body class="theme-red">

        <%- include("../../layouts/page_loader.ejs") %>
        <%- include("../../layouts/sidebars_all.ejs") %>

        <!-- #START# Content-->
        <section class="content">
            <div class="container-fluid">
                <div class="block-header">
                    <h2>Usuarios</h2><br>
                    <a href="/admin/usuarios/cadastrar">
                        <button type="button" class="btn bg-red waves-effect">Cadastrar</button>
                    </a>
                </div>

                <!-- Users Table -->
                <div class="row clearfix">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="card">
                            <div class="header">
                                <h2>
                                    USUARIOS
                                </h2>
                                <ul class="header-dropdown m-r--5">
                                    <li class="dropdown">
                                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                            <i class="material-icons">more_vert</i>
                                        </a>
                                        <ul class="dropdown-menu pull-right">
                                            <li><a href="javascript:void(0);">Action</a></li>
                                            <li><a href="javascript:void(0);">Another action</a></li>
                                            <li><a href="javascript:void(0);">Something else here</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                            <div class="body">
                                <div class="table-responsive">
                                    <table class="table table-bordered table-striped table-hover dataTable js-exportable">
                                        <thead>
                                            <tr>
                                                <th>nome</th>
                                                <th>login</th>
                                                <th>senha</th>
                                                <th>last seen</th>
                                                <th>turma</th>
                                            </tr>
                                        </thead>
                                        <tfoot>
                                            <tr>
                                                <th>nome</th>
                                                <th>login</th>
                                                <th>senha</th>
                                                <th>last seen</th>
                                                <th>turma</th>
                                            </tr>
                                        </tfoot>
                                        <tbody>
                                            <% usuarios.forEach((usuario) => { %>
                                                <tr>
                                                    <td><%- usuario.nome %></td>
                                                    <td><%- usuario.login %></td>
                                                    <td><%- usuario.senha %></td>
                                                    <td><%- usuario.last_seen %></td>
                                                    <td><%- usuario.turma %></td>
                                                </tr>
                                            <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- #END# Exportable Table -->

            </div>
        </section>
        <!-- #END# Content-->

        <!-- #START# Scripts-->
        <script src="../../plugins/jquery/jquery.min.js"></script><!-- JQuery plugin -->
        <script src="../../plugins/bootstrap/js/bootstrap.js"></script><!-- Bootstrap Core Js -->
        <script src="../../plugins/bootstrap-select/js/bootstrap-select.js"></script><!-- Select Plugin Js -->
        <script src="../../plugins/jquery-slimscroll/jquery.slimscroll.js"></script><!-- Slimscroll Plugin Js -->
        <script src="../../plugins/node-waves/waves.js"></script><!-- Waves Effect Plugin Js -->
        <script src="../../plugins/jquery-datatable/jquery.dataTables.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/skin/bootstrap/js/dataTables.bootstrap.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/dataTables.buttons.min.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/buttons.flash.min.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/jszip.min.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/vfs_fonts.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/buttons.html5.min.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/jquery-datatable/extensions/export/buttons.print.min.js"></script><!-- Jquery DataTable Plugin Js -->
        <script src="../../plugins/waitme/waitMe.js"></script><!-- Wait Me Plugin Js -->
        <script src="../../js/pages/tables/jquery-datatable.js"></script>
        <script src="../../js/admin.js"></script><!-- Custom Js -->
        <script src="../../js/pages/cards/colored.js"></script><!-- Custom Js -->
        <script src="../../js/demo.js"></script><!-- Demo Js -->
        <script src="../../socket.io/socket.io.js"></script><!-- Socket.IO -->
        <script src="../../js/socket_manager.js"></script><!-- Socket.IO -->
        <script>
            var user_data = <%- JSON.stringify(user) %>
        </script>
        <!-- #END# Scripts-->

    </body>

</html>
